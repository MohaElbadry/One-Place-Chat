# Development Environment Configuration
NODE_ENV=development
PORT=3001

# API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=sk-proj-your_openai_key_here
PROJECT_ID_OPENAI=your_project_id_here

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
ENABLE_CORS=true

# Database Configuration
DB_LOGGING=true

# ChromaDB Configuration (Local Development)
CHROMADB_HOST=localhost
CHROMADB_PORT=8000
CHROMADB_PATH=http://localhost:8000
CHROMADB_AUTO_CLEANUP=true
CHROMADB_CLEANUP_INTERVAL=24
CHROMADB_MAX_CONVERSATION_AGE=168

# Feature Flags
ENABLE_DEBUG_ROUTES=true
ENABLE_SWAGGER=true
LOG_LEVEL=debug

# Security (Development - use weak values)
JWT_SECRET=dev-jwt-secret-key-change-in-production
ENABLE_HELMET=false

# Performance
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
REQUEST_TIMEOUT_MS=30000
MAX_REQUEST_SIZE=10mb
